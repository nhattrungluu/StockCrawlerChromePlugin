{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "/assets/get_started16.png",
      "32": "/assets/get_started32.png",
      "48": "/assets/get_started48.png",
      "128": "/assets/get_started128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://iboard.ssi.com.vn/*"],
      "all_frames": true,
      "run_at": "document_end",
      "js": ["/script/crawlingStockData.js"]
    }
  ],
  "icons": {
    "16": "/assets/get_started16.png",
    "32": "/assets/get_started32.png",
    "48": "/assets/get_started48.png",
    "128": "/assets/get_started128.png"
  },
  "permissions": ["storage", "activeTab", "alarms", "tabs", "background"],
  "manifest_version": 2
}
